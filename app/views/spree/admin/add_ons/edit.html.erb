<%= render 'spree/admin/shared/configuration_menu' %>

<% content_for :page_title do %>
    Editing Design Fee
<% end %>

<% content_for :page_actions do %>
    <li><%= link_to_with_icon 'arrow-left', "Back to Design Fees", admin_add_ons_path, :class => 'button' %></li>
<% end %>


<%= render :partial => 'spree/shared/error_messages', :locals => { :target => @add_ons } %>

<%= form_for [:admin, @add_ons] do |f| %>
    <fieldset class="no-border-top">
      <%= render :partial => 'form', :locals => { :f => f } %>
      <div class="clear"></div>
      <%= render :partial => 'spree/admin/shared/edit_resource_links' %>
    </fieldset>
<% end %>